{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "declaration": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "lib": [
      "dom",
      "es6"
    ]
  },
  "include": [
    "src"
  ]
}
